package kodenst;

public class Main {
	public static void main(String[] args)
	{
	String[] arr = {"C++","C","Java","Python"};
	for(int i=0;i<arr.length;i++)
	{
	System.out.print(arr[i]+" ");
	if(arr[i].equals("Java"))
	continue;
	System.out.println("is object oriented programming language.");
	break;
	}
	}
	}