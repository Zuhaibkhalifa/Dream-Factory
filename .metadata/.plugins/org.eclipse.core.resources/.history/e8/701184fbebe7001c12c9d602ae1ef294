package com.kodnest.java.string.replacearray;

import java.util.Scanner;

public class assignment {
	        public static void main(String[] args)
	        {
	               String s1 = "KODNEST ";
	               String s2 = "TECHNOLOGIES ";
	               String s3 = s1;
	               String s4 = s2;
	               String s5 = (s3+s4).concat(s1+s2);
	               System.out.println(s5);
	        }
	}




